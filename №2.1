#include <iostream>
#include <string>
using namespace std;
int main() {
    int a,b;
    string d;
    cout <<"What operation to perform? : ";
    cin >> a >> d >> b ;
    if (d == "+"){
        cout <<a+b;
    }
    else if (d == "-"){
        cout <<a-b;
    }
    else if (d == "*"){
        cout <<a*b;
    }
    else if (d == "/"){
        cout <<a/b;
    }
}
